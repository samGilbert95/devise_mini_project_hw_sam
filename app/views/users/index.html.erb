<div class="container">
<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table class = "table">
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Prof pic</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.prof_pic %></td>
        <td><%= link_to 'Show', user , class: 'btn btn-info'%></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
</div>
