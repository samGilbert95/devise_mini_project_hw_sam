<div class="container">
<p id="notice"><%= notice %></p>

<h1>Teches</h1>

<table class="table">
  <thead>
    <tr>
      <th>Technology</th>
      <th>Desc</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teches.each do |tech| %>
      <tr>
        <td><%= tech.technology %></td>
        <td><%= tech.desc %></td>
        <td><%= link_to 'Show', tech %></td>
        <% if tech.user.id == current_user.id %>
        <td><%= link_to 'Edit', edit_tech_path(tech) %></td>
        <td><%= link_to 'Destroy', tech, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= link_to 'New Tech', new_tech_path %>
</div>
